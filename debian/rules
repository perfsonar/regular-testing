#!/usr/bin/make -f

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@ --with quilt,systemd

# Standard make install not needed
override_dh_auto_install:

override_dh_install:
	dh_install
	chmod +x $(CURDIR)/debian/perfsonar-regulartesting/usr/lib/perfsonar/bin/regulartesting.pl
